{
  "name": "jszy-swagger-doc-generator",
  "version": "1.1.1",
  "description": "A tool to generate frontend documentation from Swagger/OpenAPI JSON files",
  "main": "dist/index.js",
  "bin": {
    "swagger-doc-generator": "./dist/cli.js",
    "jszy-swagger-doc-generator": "./dist/cli.js",
    "api-sdk": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/cli.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "swagger",
    "documentation",
    "api",
    "frontend",
    "openapi"
  ],
  "author": "Swagger Doc Generator Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@types/yargs": "^17.0.24",
    "typescript": "^5.2.2",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.5",
    "ts-jest": "^29.1.1"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/__tests__"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/index.ts",
      "!src/cli.ts"
    ]
  }
}
